{
    "name": "serrvius/apiclient",
    "type": "library",
    "description": "Client library base on Google APIs",
    "keywords": ["google", "serrvius", "apiclient"],
    "homepage": "https://github.com/serrvius/apiclient",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.4",
        "google/auth": "^1.0",
        "firebase/php-jwt": "~2.0||~3.0||~4.0||~5.0",
        "monolog/monolog": "^1.17",
        "phpseclib/phpseclib": "~0.3.10||~2.0",
        "guzzlehttp/guzzle": "~5.3.1||~6.0",
        "guzzlehttp/psr7": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8.36",
        "squizlabs/php_codesniffer": "~2.3",
        "symfony/dom-crawler": "~2.1",
        "symfony/css-selector": "~2.1",
        "cache/filesystem-adapter": "^0.3.2"
    },
    "suggest": {
        "cache/filesystem-adapter": "For caching certs and tokens (using AcordTravel_Client::setCache)"
    },
    "autoload": {
        "psr-0": {
            "AcordTravel_": "src/"
        },
        "classmap": [
            "src/AcordTravel/Service/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
